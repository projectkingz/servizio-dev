<div class="container tn_top_bottom_spacer">
    <h3 align="center"> Servizio Admin Users</h3>
    <div align="center">
            <% @admin.each do |admin| %>
              <ul class = "user-listing">
                      <div class="row">
                            <div class="col-md-4">
                                        <li><%= image_tag admin_avatar(admin, 150), class: "rounded-circle mx-auto d-block" %><li>
                                                <li class="listing-title">
                                                  <%= link_to admin.username, admin_path(admin) %>
                                                </li>
                            </div>


                        </div>
                </ul>
        <% end %>
            <div class="digg_pagination">
              <div align="center">
                  <div class="page_info">
                    <%= page_entries_info @admin %>
                  </div>
                    <%= will_paginate @admin  %>
                  </div>
            </div>

    </div>
</div>
